definiere haupt(): ganzzahl {
    lasse i: ganzzahl = 20;
    waehrend(i > 0) {
      wenn (i % 2 == 0) {
        drucke("%g\n", i);
        i = i - 1;
        fahre fort;
      } ansonsten wenn (i % 3 == 0) {
        drucke("%g\n", i);
        breche;
      }
      drucke("%g\n", i);
      i = i - 1;
    }
}
