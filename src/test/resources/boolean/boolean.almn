definiere haupt(): ganzzahl {
    konstante x: wahrheitswert = wahr;
    konstante y: wahrheitswert = falsch;
    konstante n: ganzzahl = 42;
    konstante m: ganzzahl = 0;

    wenn (x und y oder n > m und falsch oder wahr) {
        drucke("x && y || n > m && false || true ist wahr\n");
    } ansonsten {
        drucke("x && y || n > m && false || true ist falsch\n");
    }

}
